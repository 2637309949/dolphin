<sqlMap>
    <sql id="insert_sys_role">
        insert into sys_role
		(`id`,`name`,`code`,`status`,`create_by`,`create_time`,`update_by`,`update_time`,`del_flag`)
		values
		(?id,?name,?code,?status,?create_by,?create_time,?update_by,?update_time,?del_flag)
    </sql>
    <sql id="update_sys_role">
        update sys_role set `id`=?id,`name`=?name,`code`=?code,`status`=?status,`create_by`=?create_by,`create_time`=?create_time,`update_by`=?update_by,`update_time`=?update_time,`del_flag`=?del_flag
		where  id =?id
    </sql>
    <sql id="delete_sys_role">
        delete from sys_role
		where id =?id
    </sql>
    <sql id="selectone_sys_role">
        select `id`,`name`,`code`,`status`,`create_by`,`create_time`,`update_by`,`update_time`,`del_flag` from sys_role
		where  id =?id
    </sql>
    <sql id="selectall_sys_role">
        select `id`,`name`,`code`,`status`,`create_by`,`create_time`,`update_by`,`update_time`,`del_flag` from sys_role
    </sql>
</sqlMap>
