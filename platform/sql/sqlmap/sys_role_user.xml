<sqlMap>
    <sql id="insert_sys_role_user">
        insert into sys_role_user
		(`id`,`user_id`,`role_id`,`create_by`,`create_time`,`update_by`,`update_time`,`del_flag`)
		values
		(?id,?user_id,?role_id,?create_by,?create_time,?update_by,?update_time,?del_flag)
    </sql>
    <sql id="update_sys_role_user">
        update sys_role_user set `id`=?id,`user_id`=?user_id,`role_id`=?role_id,`create_by`=?create_by,`create_time`=?create_time,`update_by`=?update_by,`update_time`=?update_time,`del_flag`=?del_flag
		where  id =?id
    </sql>
    <sql id="delete_sys_role_user">
        delete from sys_role_user
		where id =?id
    </sql>
    <sql id="selectone_sys_role_user">
        select `id`,`user_id`,`role_id`,`create_by`,`create_time`,`update_by`,`update_time`,`del_flag` from sys_role_user
		where  id =?id
    </sql>
    <sql id="selectall_sys_role_user">
        select `id`,`user_id`,`role_id`,`create_by`,`create_time`,`update_by`,`update_time`,`del_flag` from sys_role_user
    </sql>
</sqlMap>
