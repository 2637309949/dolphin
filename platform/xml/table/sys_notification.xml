<table name="sys_notification" desc="站内消息" packages="github.com/2637309949/dolphin/packages/null">
    <column name="id" desc="主键" type="null.String" xorm="varchar(36) notnull unique pk" />
    <column name="recipient_id" desc="接受者" type="null.String" xorm="varchar(36)" />
    <column name="sender_id" desc="发送者" type="null.String" xorm="varchar(36)" />
    <column name="unread" desc="是否已读" type="null.Int" xorm="notnull" />
    <column name="type" desc="消息类型" type="null.Int" xorm="notnull" />
    <column name="parameters" desc="额外数据" type="null.String" xorm="text" />
    <column name="reference_id" desc="对象引用" type="null.String" xorm="varchar(36)" />

	<column name="create_by" desc="创建人" type="null.String" xorm="varchar(36) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime notnull" />
	<column name="update_by" desc="最后更新人" type="null.String" xorm="varchar(36) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime notnull" />
    <column name="del_flag" desc="删除标记" type="null.Int" xorm="notnull" />
    <column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>