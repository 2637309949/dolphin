<controller name="sys_tracker" desc="日志">
    <api name="page" func="page" table="sys_tracker" desc="日志分页查询" method="get">
        <param name="page" type="int" desc="页码" value="1"/>
        <param name="size" type="int" desc="单页数" value="15" />
        <param name="app_name" type="string" desc="所属应用"/>
        <return>
            <success type="$success"/>
            <failure type="$fail"/>
        </return>
    </api>
    <api name="get" desc="获取日志信息" func="one" table="sys_tracker" method="get">
        <param name="id" type="string" desc="日志id" />
        <return>
            <success type="$success"/>
            <failure type="$fail"/>
        </return>
    </api>
</controller>