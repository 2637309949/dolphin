<controller name="sys_optionset" desc="字典">
    <api name="add" function="add" table="sys_optionset" desc="添加字典" method="post">
        <param name="user" type="$sys_optionset" desc="字典信息" />
        <return>
            <success type="$response"/>
            <failure type="$response"/>
        </return>
    </api>
    <api name="del" function="delete" table="sys_optionset" desc="删除字典" method="delete">
        <param name="sys_optionset" type="$sys_optionset" desc="字典" />
        <return>
            <success type="$response"/>
            <failure type="$response"/>
        </return>
    </api>
    <api name="update" function="update" table="sys_optionset" desc="更新字典" method="put">
        <param name="user" type="$sys_role" desc="字典信息" />
        <return>
            <success type="$response"/>
            <failure type="$response"/>
        </return>
    </api>
    <api name="page" function="page" table="sys_optionset" desc="字典分页查询" method="get">
        <param name="page" type="int" desc="页码" value="1"/>
        <param name="size" type="int" desc="单页数" value="15" />
        <return>
            <success type="$response"/>
            <failure type="$response"/>
        </return>
    </api>
    <api name="get" desc="获取字典信息" function="one" table="sys_optionset" method="get">
        <param name="id" type="string" desc="字典id" />
        <param name="code" type="string" desc="字典code" />
        <return>
            <success type="$response"/>
            <failure type="$response"/>
        </return>
    </api>
</controller>