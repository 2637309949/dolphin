<controller name="sys_optionset" desc="字典">
    <api name="add" func="add" table="sys_optionset" method="post" desc="添加字典">
        <param name="user" type="$sys_optionset" desc="字典信息" />
        <return>
            <success type="$success"/>
            <failure type="$fail"/>
        </return>
    </api>
    <api name="del" func="delete" table="sys_optionset" method="delete" desc="删除字典">
        <param name="sys_optionset" type="$sys_optionset" desc="字典" />
        <return>
            <success type="$success"/>
            <failure type="$fail"/>
        </return>
    </api>
    <api name="update" func="update" table="sys_optionset" method="put" desc="更新字典">
        <param name="user" type="$sys_role" desc="字典信息" />
        <return>
            <success type="$success"/>
            <failure type="$fail"/>
        </return>
    </api>
    <api name="page" func="page" table="sys_optionset" method="get" desc="字典分页查询">
        <param name="page" type="int" value="1" desc="页码"/>
        <param name="size" type="int"  value="15" desc="单页数"/>
        <return>
            <success type="$success"/>
            <failure type="$fail"/>
        </return>
    </api>
    <api name="get" func="one" table="sys_optionset" method="get" desc="获取字典信息">
        <param name="id" type="string" desc="字典id" />
        <param name="code" type="string" desc="字典code" />
        <return>
            <success type="$success"/>
            <failure type="$fail"/>
        </return>
    </api>
</controller>