
<sqlMap>
    <sql id="insert_material_apply">
        insert into material_apply
		(`m_a_id`,`mt_id`,`ma_num`,`ma_money`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`mb_id`)
		values
		(?m_a_id,?mt_id,?ma_num,?ma_money,?creater,?create_date,?updater,?update_date,?is_delete,?mb_id)
    </sql>
    <sql id="update_material_apply">
        update material_apply set `m_a_id`=?m_a_id,`mt_id`=?mt_id,`ma_num`=?ma_num,`ma_money`=?ma_money,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete,`mb_id`=?mb_id
		where id =?id
    </sql>
    <sql id="deleteone_material_apply">
        delete from material_apply where id =?id
    </sql>
    <sql id="selectone_material_apply">
        select `m_a_id`,`mt_id`,`ma_num`,`ma_money`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`mb_id` from material_apply where id =?id
    </sql>
    <sql id="selectall_material_apply">
        select `m_a_id`,`mt_id`,`ma_num`,`ma_money`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`mb_id` from material_apply
    </sql>
</sqlMap>
