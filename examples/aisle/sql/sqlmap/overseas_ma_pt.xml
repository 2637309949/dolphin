
<sqlMap>
    <sql id="insert_overseas_ma_pt">
        insert into overseas_ma_pt
		(`o_m_p_id`,`ma_id`,`pt_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?o_m_p_id,?ma_id,?pt_id,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_overseas_ma_pt">
        update overseas_ma_pt set `o_m_p_id`=?o_m_p_id,`ma_id`=?ma_id,`pt_id`=?pt_id,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_overseas_ma_pt">
        delete from overseas_ma_pt where id =?id
    </sql>
    <sql id="selectone_overseas_ma_pt">
        select `o_m_p_id`,`ma_id`,`pt_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from overseas_ma_pt where id =?id
    </sql>
    <sql id="selectall_overseas_ma_pt">
        select `o_m_p_id`,`ma_id`,`pt_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from overseas_ma_pt
    </sql>
</sqlMap>
