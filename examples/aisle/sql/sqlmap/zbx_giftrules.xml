
<sqlMap>
    <sql id="insert_zbx_giftrules">
        insert into zbx_giftrules
		(`z_g_id`,`bx_type`,`zbx_desc`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?z_g_id,?bx_type,?zbx_desc,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_zbx_giftrules">
