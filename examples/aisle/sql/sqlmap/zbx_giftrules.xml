		where id =?id
    </sql>
    <sql id="deleteone_zbx_giftrules">
        delete from zbx_giftrules where id =?id
    </sql>
    <sql id="selectone_zbx_giftrules">
        select `z_g_id`,`bx_type`,`zbx_desc`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from zbx_giftrules where id =?id
    </sql>
    <sql id="selectall_zbx_giftrules">
