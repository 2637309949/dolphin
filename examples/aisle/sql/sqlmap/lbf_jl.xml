
<sqlMap>
    <sql id="insert_lbf_jl">
        insert into lbf_jl
		(`lbf_jl_id`,`lbf_id`,`bf_date`,`bf_sc`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`stu_id`)
		values
		(?lbf_jl_id,?lbf_id,?bf_date,?bf_sc,?creater,?create_date,?updater,?update_date,?is_delete,?stu_id)
    </sql>
    <sql id="update_lbf_jl">
