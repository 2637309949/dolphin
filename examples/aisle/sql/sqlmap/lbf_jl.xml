		where id =?id
    </sql>
    <sql id="deleteone_lbf_jl">
        delete from lbf_jl where id =?id
    </sql>
    <sql id="selectone_lbf_jl">
        select `lbf_jl_id`,`lbf_id`,`bf_date`,`bf_sc`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`stu_id` from lbf_jl where id =?id
    </sql>
    <sql id="selectall_lbf_jl">
