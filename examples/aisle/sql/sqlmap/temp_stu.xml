
<sqlMap>
    <sql id="insert_temp_stu">
        insert into temp_stu
		(`fid`,`stu_name`,`stu_phone`)
		values
		(?fid,?stu_name,?stu_phone)
    </sql>
    <sql id="update_temp_stu">
        update temp_stu set `fid`=?fid,`stu_name`=?stu_name,`stu_phone`=?stu_phone
		where id =?id
    </sql>
    <sql id="deleteone_temp_stu">
        delete from temp_stu where id =?id
    </sql>
    <sql id="selectone_temp_stu">
        select `fid`,`stu_name`,`stu_phone` from temp_stu where id =?id
    </sql>
    <sql id="selectall_temp_stu">
        select `fid`,`stu_name`,`stu_phone` from temp_stu
    </sql>
</sqlMap>
