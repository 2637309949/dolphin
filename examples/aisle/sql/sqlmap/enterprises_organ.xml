
<sqlMap>
    <sql id="insert_enterprises_organ">
        insert into enterprises_organ
		(`e_o_id`,`create_date`,`update_date`,`ent_id`,`organ_id`,`creater`,`updater`,`is_delete`)
		values
		(?e_o_id,?create_date,?update_date,?ent_id,?organ_id,?creater,?updater,?is_delete)
    </sql>
    <sql id="update_enterprises_organ">
        update enterprises_organ set `e_o_id`=?e_o_id,`create_date`=?create_date,`update_date`=?update_date,`ent_id`=?ent_id,`organ_id`=?organ_id,`creater`=?creater,`updater`=?updater,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_enterprises_organ">
        delete from enterprises_organ where id =?id
    </sql>
    <sql id="selectone_enterprises_organ">
        select `e_o_id`,`create_date`,`update_date`,`ent_id`,`organ_id`,`creater`,`updater`,`is_delete` from enterprises_organ where id =?id
    </sql>
    <sql id="selectall_enterprises_organ">
        select `e_o_id`,`create_date`,`update_date`,`ent_id`,`organ_id`,`creater`,`updater`,`is_delete` from enterprises_organ
    </sql>
</sqlMap>
