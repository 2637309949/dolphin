
<sqlMap>
    <sql id="insert_channel_plan">
        insert into channel_plan
		(`c_p_id`,`pk_sch`,`pk_user`,`in_paln`,`out_plan`,`demo_plan`,`paln_month`,`achievement_plan`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?c_p_id,?pk_sch,?pk_user,?in_paln,?out_plan,?demo_plan,?paln_month,?achievement_plan,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_channel_plan">
        update channel_plan set `c_p_id`=?c_p_id,`pk_sch`=?pk_sch,`pk_user`=?pk_user,`in_paln`=?in_paln,`out_plan`=?out_plan,`demo_plan`=?demo_plan,`paln_month`=?paln_month,`achievement_plan`=?achievement_plan,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_channel_plan">
        delete from channel_plan where id =?id
    </sql>
    <sql id="selectone_channel_plan">
        select `c_p_id`,`pk_sch`,`pk_user`,`in_paln`,`out_plan`,`demo_plan`,`paln_month`,`achievement_plan`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from channel_plan where id =?id
    </sql>
    <sql id="selectall_channel_plan">
