
<sqlMap>
    <sql id="insert_android_hotfix">
        insert into android_hotfix
		(`a_h_id`,`app_name`,`applicationid`,`version_name`,`descriptions`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?a_h_id,?app_name,?applicationid,?version_name,?descriptions,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_android_hotfix">
        update android_hotfix set `a_h_id`=?a_h_id,`app_name`=?app_name,`applicationid`=?applicationid,`version_name`=?version_name,`descriptions`=?descriptions,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_android_hotfix">
        delete from android_hotfix where id =?id
    </sql>
    <sql id="selectone_android_hotfix">
        select `a_h_id`,`app_name`,`applicationid`,`version_name`,`descriptions`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from android_hotfix where id =?id
    </sql>
    <sql id="selectall_android_hotfix">
        select `a_h_id`,`app_name`,`applicationid`,`version_name`,`descriptions`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from android_hotfix
    </sql>
</sqlMap>
