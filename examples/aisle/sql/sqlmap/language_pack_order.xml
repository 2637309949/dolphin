		where id =?id
    </sql>
    <sql id="deleteone_language_pack_order">
        delete from language_pack_order where id =?id
    </sql>
    <sql id="selectone_language_pack_order">
        select `l_p_o_id`,`lp_id`,`order_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from language_pack_order where id =?id
    </sql>
    <sql id="selectall_language_pack_order">
