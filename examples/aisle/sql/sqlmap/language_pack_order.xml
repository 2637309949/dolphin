
<sqlMap>
    <sql id="insert_language_pack_order">
        insert into language_pack_order
		(`l_p_o_id`,`lp_id`,`order_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?l_p_o_id,?lp_id,?order_id,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_language_pack_order">
