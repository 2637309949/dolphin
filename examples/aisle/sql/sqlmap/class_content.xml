
<sqlMap>
    <sql id="insert_class_content">
        insert into class_content
		(`c_c_id`,`cs_id`,`class_content`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?c_c_id,?cs_id,?class_content,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_class_content">
        update class_content set `c_c_id`=?c_c_id,`cs_id`=?cs_id,`class_content`=?class_content,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_class_content">
        delete from class_content where id =?id
    </sql>
    <sql id="selectone_class_content">
        select `c_c_id`,`cs_id`,`class_content`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from class_content where id =?id
    </sql>
    <sql id="selectall_class_content">
