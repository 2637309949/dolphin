
<sqlMap>
    <sql id="insert_cpc_city">
        insert into cpc_city
		(`cpc_city_id`,`cpc_id`,`city_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?cpc_city_id,?cpc_id,?city_id,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_cpc_city">
        update cpc_city set `cpc_city_id`=?cpc_city_id,`cpc_id`=?cpc_id,`city_id`=?city_id,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_cpc_city">
        delete from cpc_city where id =?id
    </sql>
    <sql id="selectone_cpc_city">
        select `cpc_city_id`,`cpc_id`,`city_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from cpc_city where id =?id
    </sql>
    <sql id="selectall_cpc_city">
        select `cpc_city_id`,`cpc_id`,`city_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from cpc_city
    </sql>
</sqlMap>
