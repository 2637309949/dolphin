
<sqlMap>
    <sql id="insert_train_organ">
        insert into train_organ
		(`t_o_id`,`train_id`,`organ_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?t_o_id,?train_id,?organ_id,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_train_organ">
        update train_organ set `t_o_id`=?t_o_id,`train_id`=?train_id,`organ_id`=?organ_id,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_train_organ">
        delete from train_organ where id =?id
    </sql>
    <sql id="selectone_train_organ">
        select `t_o_id`,`train_id`,`organ_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from train_organ where id =?id
    </sql>
    <sql id="selectall_train_organ">
        select `t_o_id`,`train_id`,`organ_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from train_organ
    </sql>
</sqlMap>
