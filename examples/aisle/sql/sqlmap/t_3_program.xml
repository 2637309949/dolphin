
<sqlMap>
    <sql id="insert_t_3_program">
        insert into t_3_program
		(`pro_id`,`pro_name`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?pro_id,?pro_name,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_t_3_program">
        update t_3_program set `pro_id`=?pro_id,`pro_name`=?pro_name,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_t_3_program">
        delete from t_3_program where id =?id
    </sql>
    <sql id="selectone_t_3_program">
        select `pro_id`,`pro_name`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from t_3_program where id =?id
    </sql>
    <sql id="selectall_t_3_program">
        select `pro_id`,`pro_name`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from t_3_program
    </sql>
</sqlMap>
