
<sqlMap>
    <sql id="insert_sheng">
        insert into sheng
		(`sheng_id`,`sheng_name`,`sheng_code`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`sheng_area`)
		values
		(?sheng_id,?sheng_name,?sheng_code,?creater,?create_date,?updater,?update_date,?is_delete,?sheng_area)
    </sql>
    <sql id="update_sheng">
        update sheng set `sheng_id`=?sheng_id,`sheng_name`=?sheng_name,`sheng_code`=?sheng_code,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete,`sheng_area`=?sheng_area
		where id =?id
    </sql>
    <sql id="deleteone_sheng">
        delete from sheng where id =?id
    </sql>
    <sql id="selectone_sheng">
        select `sheng_id`,`sheng_name`,`sheng_code`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`sheng_area` from sheng where id =?id
    </sql>
    <sql id="selectall_sheng">
        select `sheng_id`,`sheng_name`,`sheng_code`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`,`sheng_area` from sheng
    </sql>
</sqlMap>
