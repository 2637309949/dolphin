
<sqlMap>
    <sql id="insert_rps_sch">
        insert into rps_sch
		(`rps_sch_id`,`rps_id`,`sch_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete`)
		values
		(?rps_sch_id,?rps_id,?sch_id,?creater,?create_date,?updater,?update_date,?is_delete)
    </sql>
    <sql id="update_rps_sch">
        update rps_sch set `rps_sch_id`=?rps_sch_id,`rps_id`=?rps_id,`sch_id`=?sch_id,`creater`=?creater,`create_date`=?create_date,`updater`=?updater,`update_date`=?update_date,`is_delete`=?is_delete
		where id =?id
    </sql>
    <sql id="deleteone_rps_sch">
        delete from rps_sch where id =?id
    </sql>
    <sql id="selectone_rps_sch">
        select `rps_sch_id`,`rps_id`,`sch_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from rps_sch where id =?id
    </sql>
    <sql id="selectall_rps_sch">
        select `rps_sch_id`,`rps_id`,`sch_id`,`creater`,`create_date`,`updater`,`update_date`,`is_delete` from rps_sch
    </sql>
</sqlMap>
