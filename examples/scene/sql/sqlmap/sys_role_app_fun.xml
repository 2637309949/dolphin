
<sqlMap>
    <sql id="insert_sys_role_app_fun">
        insert into sys_role_app_fun
		(`id`,`role_id`,`app_fun_id`,`creater`,`create_time`,`updater`,`update_time`,`is_delete`,`remark`)
		values
		(?id,?role_id,?app_fun_id,?creater,?create_time,?updater,?update_time,?is_delete,?remark)
    </sql>
    <sql id="update_sys_role_app_fun">
