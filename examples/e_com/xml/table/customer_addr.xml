<table name="customer_addr" desc="用户地址表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="customer_addr_id" desc="自增主键ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="customer_id" desc="customer_login表的自增ID" type="null.Int" xorm="int(10) notnull" />
	<column name="zip" desc="邮编" type="null.Int" xorm=" notnull" />
	<column name="province" desc="地区表中省份的ID" type="null.Int" xorm=" notnull" />
	<column name="city" desc="地区表中城市的ID" type="null.Int" xorm=" notnull" />
	<column name="district" desc="地区表中的区ID" type="null.Int" xorm=" notnull" />
	<column name="address" desc="具体的地址门牌号" type="null.String" xorm="varchar(200) notnull" />
	<column name="is_default" desc="是否默认" type="null.Int" xorm=" notnull" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>