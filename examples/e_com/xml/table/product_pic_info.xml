<table name="product_pic_info" desc="商品图片信息表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="product_pic_id" desc="商品图片ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="product_id" desc="商品ID" type="null.Int" xorm="int(10) notnull" />
	<column name="pic_desc" desc="图片描述" type="null.String" xorm="varchar(50)" />
	<column name="pic_url" desc="图片URL" type="null.String" xorm="varchar(200) notnull" />
	<column name="is_master" desc="是否主图：0.非主图1.主图" type="null.Int" xorm=" notnull default(0)" />
	<column name="pic_order" desc="图片排序" type="null.Int" xorm=" notnull default(0)" />
	<column name="pic_status" desc="图片是否有效：0无效 1有效" type="null.Int" xorm=" notnull default(1)" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>