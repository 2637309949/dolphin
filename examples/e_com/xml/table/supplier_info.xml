<table name="supplier_info" desc="供应商信息表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="supplier_id" desc="供应商ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="supplier_code" desc="供应商编码" type="null.String" xorm=" notnull" />
	<column name="supplier_name" desc="供应商名称" type="null.String" xorm=" notnull" />
	<column name="supplier_type" desc="供应商类型：1.自营，2.平台" type="null.Int" xorm=" notnull" />
	<column name="link_man" desc="供应商联系人" type="null.String" xorm="varchar(10) notnull" />
	<column name="phone_number" desc="联系电话" type="null.String" xorm="varchar(50) notnull" />
	<column name="bank_name" desc="供应商开户银行名称" type="null.String" xorm="varchar(50) notnull" />
	<column name="bank_account" desc="银行账号" type="null.String" xorm="varchar(50) notnull" />
	<column name="address" desc="供应商地址" type="null.String" xorm="varchar(200) notnull" />
	<column name="supplier_status" desc="状态：0禁止，1启用" type="null.Int" xorm=" notnull default(0)" />
	
	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>