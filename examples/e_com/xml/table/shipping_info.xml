<table name="shipping_info" desc="物流公司信息表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="ship_id" desc="主键ID" type="null.Int" xorm=" pk notnull autoincr" />
	<column name="ship_name" desc="物流公司名称" type="null.String" xorm="varchar(20) notnull" />
	<column name="ship_contact" desc="物流公司联系人" type="null.String" xorm="varchar(20) notnull" />
	<column name="telephone" desc="物流公司联系电话" type="null.String" xorm="varchar(20) notnull" />
	<column name="price" desc="配送价格" type="decimal.Decimal" xorm="decimal(8,2) notnull default(0.00)" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>