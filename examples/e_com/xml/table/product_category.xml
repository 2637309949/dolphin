<table name="product_category" desc="商品分类表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="category_id" desc="分类ID" type="null.Int" xorm=" pk notnull autoincr" />
	<column name="category_name" desc="分类名称" type="null.String" xorm="varchar(10) notnull" />
	<column name="category_code" desc="分类编码" type="null.String" xorm="varchar(10) notnull" />
	<column name="parent_id" desc="父分类ID" type="null.Int" xorm=" notnull default(0)" />
	<column name="category_level" desc="分类层级" type="null.Int" xorm=" notnull default(1)" />
	<column name="category_status" desc="分类状态" type="null.Int" xorm=" notnull default(1)" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>