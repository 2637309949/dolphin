<table name="product_info" desc="商品信息表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="product_id" desc="商品ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="product_core" desc="商品编码" type="null.String" xorm=" notnull" />
	<column name="product_name" desc="商品名称" type="null.String" xorm="varchar(20) notnull" />
	<column name="bar_code" desc="国条码" type="null.String" xorm="varchar(50) notnull" />
	<column name="brand_id" desc="品牌表的ID" type="null.Int" xorm="int(10) notnull" />
	<column name="one_category_id" desc="一级分类ID" type="null.Int" xorm=" notnull" />
	<column name="two_category_id" desc="二级分类ID" type="null.Int" xorm=" notnull" />
	<column name="three_category_id" desc="三级分类ID" type="null.Int" xorm=" notnull" />
	<column name="supplier_id" desc="商品的供应商ID" type="null.Int" xorm="int(10) notnull" />
	<column name="price" desc="商品销售价格" type="decimal.Decimal" xorm="decimal(8,2) notnull" />
	<column name="average_cost" desc="商品加权平均成本" type="decimal.Decimal" xorm="decimal(18,2) notnull" />
	<column name="publish_status" desc="上下架状态：0下架1上架" type="null.Int" xorm=" notnull default(0)" />
	<column name="audit_status" desc="审核状态：0未审核，1已审核" type="null.Int" xorm=" notnull default(0)" />
	<column name="weight" desc="商品重量" type="null.Float" xorm="float" />
	<column name="length" desc="商品长度" type="null.Float" xorm="float" />
	<column name="height" desc="商品高度" type="null.Float" xorm="float" />
	<column name="width" desc="商品宽度" type="null.Float" xorm="float" />
	<column name="color_type" desc="" type="null.Int" xorm="int(10)" />
	<column name="production_date" desc="生产日期" type="null.Time" xorm="datetime notnull" />
	<column name="shelf_life" desc="商品有效期" type="null.Int" xorm="int(11) notnull" />
	<column name="descript" desc="商品描述" type="null.String" xorm="text(0) notnull" />
	<column name="indate" desc="商品录入时间" type="null.Time" xorm=" notnull default(CURRENT_TIMESTAMP)" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>