<table name="customer_login" desc="用户登录表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="customer_id" desc="用户ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="login_name" desc="用户登录名" type="null.String" xorm="varchar(20) notnull" />
	<column name="password" desc="md5加密的密码" type="null.String" xorm=" notnull" />
	<column name="user_stats" desc="用户状态" type="null.Int" xorm=" notnull default(1)" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>