<table name="warehouse_info" desc="仓库信息表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="w_id" desc="仓库ID" type="null.Int" xorm=" pk notnull autoincr" />
	<column name="warehouse_sn" desc="仓库编码" type="null.String" xorm=" notnull" />
	<column name="warehoust_name" desc="仓库名称" type="null.String" xorm="varchar(10) notnull" />
	<column name="warehouse_phone" desc="仓库电话" type="null.String" xorm="varchar(20) notnull" />
	<column name="contact" desc="仓库联系人" type="null.String" xorm="varchar(10) notnull" />
	<column name="province" desc="省" type="null.Int" xorm=" notnull" />
	<column name="city" desc="市" type="null.Int" xorm=" notnull" />
	<column name="distrct" desc="区" type="null.Int" xorm=" notnull" />
	<column name="address" desc="仓库地址" type="null.String" xorm="varchar(100) notnull" />
	<column name="warehouse_status" desc="仓库状态：0禁用，1启用" type="null.Int" xorm=" notnull default(1)" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>