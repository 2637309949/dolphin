<table name="warehouse_product" desc="商品库存表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="wp_id" desc="商品库存ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="product_id" desc="商品ID" type="null.Int" xorm="int(10) notnull" />
	<column name="w_id" desc="仓库ID" type="null.Int" xorm=" notnull" />
	<column name="current_cnt" desc="当前商品数量" type="null.Int" xorm="int(10) notnull default(0)" />
	<column name="lock_cnt" desc="当前占用数据" type="null.Int" xorm="int(10) notnull default(0)" />
	<column name="in_transit_cnt" desc="在途数据" type="null.Int" xorm="int(10) notnull default(0)" />
	<column name="average_cost" desc="移动加权成本" type="decimal.Decimal" xorm="decimal(8,2) notnull default(0.00)" />
	
	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>