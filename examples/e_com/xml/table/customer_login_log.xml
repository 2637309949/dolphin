<table name="customer_login_log" desc="用户登陆日志表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="login_id" desc="登陆日志ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="customer_id" desc="登陆用户ID" type="null.Int" xorm="int(10) notnull" />
	<column name="login_time" desc="用户登陆时间" type="null.Time" xorm=" notnull default(CURRENT_TIMESTAMP)" />
	<column name="login_ip" desc="登陆IP" type="null.Int" xorm="int(10) notnull" />
	<column name="login_type" desc="登陆类型：0未成功，1成功" type="null.Int" xorm=" notnull" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>