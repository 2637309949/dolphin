<table name="order_detail" desc="订单详情表" packages="github.com/2637309949/dolphin/packages/null,github.com/shopspring/decimal">
	<column name="order_detail_id" desc="订单详情表ID" type="null.Int" xorm="int(10) pk notnull autoincr" />
	<column name="order_id" desc="订单表ID" type="null.Int" xorm="int(10) notnull" />
	<column name="product_id" desc="订单商品ID" type="null.Int" xorm="int(10) notnull" />
	<column name="product_name" desc="商品名称" type="null.String" xorm="varchar(50) notnull" />
	<column name="product_cnt" desc="购买商品数量" type="null.Int" xorm="int(11) notnull default(1)" />
	<column name="product_price" desc="购买商品单价" type="decimal.Decimal" xorm="decimal(8,2) notnull" />
	<column name="average_cost" desc="平均成本价格" type="decimal.Decimal" xorm="decimal(8,2) notnull" />
	<column name="weight" desc="商品重量" type="null.Float" xorm="float" />
	<column name="fee_money" desc="优惠分摊金额" type="decimal.Decimal" xorm="decimal(8,2) notnull default(0.00)" />
	<column name="w_id" desc="仓库ID" type="null.Int" xorm="int(10) notnull" />

	<column name="creater" desc="创建人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="create_time" desc="创建时间" type="null.Time" xorm="datetime" />
	<column name="updater" desc="最后更新人" type="null.Int" xorm="bigint(20) notnull" />
	<column name="update_time" desc="最后更新时间" type="null.Time" xorm="datetime" />
	<column name="is_delete" desc="删除标记" type="null.Int" xorm="notnull" />
	<column name="remark" desc="备注" type="null.String" xorm="varchar(200)" />
</table>