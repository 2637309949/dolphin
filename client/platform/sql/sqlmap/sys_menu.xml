<sqlMap>
    <sql id="insert_sys_menu">
        insert into sys_menu
		(`id`,`name`,`code`,`parent`,`inheritance`,`url`,`component`,`perms`,`type`,`icon`,`order_num`,`create_by`,`create_time`,`update_by`,`update_time`,`delete_by`,`delete_time`)
		values
		(?id,?name,?code,?parent,?inheritance,?url,?component,?perms,?type,?icon,?order_num,?create_by,?create_time,?update_by,?update_time,?delete_by,?delete_time)
    </sql>
    <sql id="update_sys_menu">
        update sys_menu set `id`=?id,`name`=?name,`code`=?code,`parent`=?parent,`inheritance`=?inheritance,`url`=?url,`component`=?component,`perms`=?perms,`type`=?type,`icon`=?icon,`order_num`=?order_num,`create_by`=?create_by,`create_time`=?create_time,`update_by`=?update_by,`update_time`=?update_time,`delete_by`=?delete_by,`delete_time`=?delete_time
