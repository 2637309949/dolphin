    </sql>
    <sql id="delete_sys_menu">
        delete from sys_menu
		where id =?id
    </sql>
    <sql id="selectone_sys_menu">
        select `id`,`name`,`code`,`parent`,`inheritance`,`url`,`component`,`perms`,`type`,`icon`,`order_num`,`create_by`,`create_time`,`update_by`,`update_time`,`delete_by`,`delete_time` from sys_menu
		where  id =?id
