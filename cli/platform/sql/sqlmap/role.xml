<sqlMap>
    <sql id="insert_platform_role">
        insert into platform_role
		(`id`,`name`,`code`,`status`,`domain`,`create_by`,`create_time`,`update_by`,`update_time`,`delete_by`,`delete_time`)
		values
		(?id,?name,?code,?status,?domain,?create_by,?create_time,?update_by,?update_time,?delete_by,?delete_time)
    </sql>
    <sql id="update_platform_role">
        update platform_role set `id`=?id,`name`=?name,`code`=?code,`status`=?status,`domain`=?domain,`create_by`=?create_by,`create_time`=?create_time,`update_by`=?update_by,`update_time`=?update_time,`delete_by`=?delete_by,`delete_time`=?delete_time
		where  id =?id
    </sql>
    <sql id="delete_platform_role">
        delete from platform_role
		where id =?id
    </sql>
    <sql id="selectone_platform_role">
        select `id`,`name`,`code`,`status`,`domain`,`create_by`,`create_time`,`update_by`,`update_time`,`delete_by`,`delete_time` from platform_role
		where  id =?id
    </sql>
    <sql id="selectall_platform_role">
        select `id`,`name`,`code`,`status`,`domain`,`create_by`,`create_time`,`update_by`,`update_time`,`delete_by`,`delete_time` from platform_role
    </sql>
</sqlMap>
