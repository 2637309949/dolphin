<sqlMap>
    <sql id="insert_platform_sys_data_permission">
        insert into platform_sys_data_permission
		(`id`,`name`,`code`,`create_by`,`create_time`,`update_by`,`update_time`,`delete_by`,`delete_time`)
		values
		(?id,?name,?code,?create_by,?create_time,?update_by,?update_time,?delete_by,?delete_time)
    </sql>
    <sql id="update_platform_sys_data_permission">
        update platform_sys_data_permission set `id`=?id,`name`=?name,`code`=?code,`create_by`=?create_by,`create_time`=?create_time,`update_by`=?update_by,`update_time`=?update_time,`delete_by`=?delete_by,`delete_time`=?delete_time
		where  id =?id
    </sql>
    <sql id="delete_platform_sys_data_permission">
        delete from platform_sys_data_permission
		where id =?id
    </sql>
    <sql id="selectone_platform_sys_data_permission">
