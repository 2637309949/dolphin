<sqlMap>
    <sql id="insert_platform_sys_tag">
        insert into platform_sys_tag
		(`id`,`name`,`group_id`,`create_by`,`create_time`,`update_by`,`update_time`,`delete_by`,`delete_time`)
		values
		(?id,?name,?group_id,?create_by,?create_time,?update_by,?update_time,?delete_by,?delete_time)
    </sql>
    <sql id="update_platform_sys_tag">
